  object qryCardClientes: TFDQuery
    Connection = FDConnection
    SQL.Strings = (
      'SELECT'
      '  c.CODIGO        AS CLIENTE_ID,'
      '  c.NOME           AS CLIENTE_NOME,'
      '  c.TELEFONE,'
      '  c.ENDERECO,'
      '  c.BAIRRO,'
      '  c.ESTADO,'
      ''
      '  cc.CODIGO       AS CARD_ID,'
      '  cc.VALOR        AS SALDO,'
      '  cc.ATIVO'
      'FROM TB_CLIENTES c'
      'INNER JOIN TB_CARDCLIENTE cc'
      '        ON cc.CLIENTE = c.CODIGO'
      'WHERE'
      '  c.ATIVO = 1'
      '  AND cc.ATIVO = 1'
      'ORDER BY c.NOME;')
    Left = 328
    Top = 256
  end
